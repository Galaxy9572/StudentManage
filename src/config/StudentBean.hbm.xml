<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="domain">
  <class name="StudentBean" table="Student" catalog="StudentManage">
     <id name="stuNum" type="java.lang.String">
         <column name="stuNum" length="10"></column>
     </id>
     <property name="stuName" type="java.lang.String">
        <column name="stuName" length="10" />
     </property>
     <property name="gender" type="java.lang.String">
        <column name="gender" length="10" />
     </property>
     <property name="age" type="java.lang.Integer">
        <column name="age" length="11" />
     </property>
     <property name="password" type="java.lang.String">
        <column name="password" length="16" />
     </property>
     <property name="selected" type="java.lang.String">
        <column name="selected" length="255" />
     </property>
     
  </class>
</hibernate-mapping>