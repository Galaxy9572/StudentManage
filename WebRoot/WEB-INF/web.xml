<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>StudentManage</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>RegLoginServlet</servlet-name>
  	<servlet-class>controller.RegLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegLoginServlet</servlet-name>
  	<url-pattern>/RegLoginServlet</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<servlet-name>StudentOperateServlet</servlet-name>
  	<servlet-class>controller.StudentOperateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>StudentOperateServlet</servlet-name>
  	<url-pattern>/StudentOperateServlet</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<servlet-name>ClassOperateServlet</servlet-name>
  	<servlet-class>controller.ClassOperateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ClassOperateServlet</servlet-name>
  	<url-pattern>/ClassOperateServlet</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<servlet-name>TeacherOperateServlet</servlet-name>
  	<servlet-class>controller.TeacherOperateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>TeacherOperateServlet</servlet-name>
  	<url-pattern>/TeacherOperateServlet</url-pattern>
  </servlet-mapping>
</web-app>